<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <link rel="stylesheet" type="text/css" href="http://cdn.datatables.net/1.10.15/css/jquery.dataTables.min.css" />
    <script type="text/javascript" src="http://cdn.datatables.net/1.10.15/js/jquery.dataTables.min.js"></script>
    <script>
        $(document).ready(function () {
            $('#foodTable').DataTable();
        });
    </script>
    <!-- ====== Bootstrap js ======= -->
    <script type="text/javascript" src="js/bootstrap.min.js"></script>
    <!-- ====== slicknav js ======= -->
    <script type="text/javascript" src="js/jquery.slicknav.min.js"></script>
    <!-- ====== modernizr js ======= -->
    <!-- ====== parallax js ======= -->
    <script type="text/javascript" src="js/jquery.parallax-1.1.3.js"></script>
    <!-- ====== circle progress js ======= -->
    <script type="text/javascript" src="js/circle-progress.js"></script>
    <!-- ====== slick js ======= -->
    <script type="text/javascript" src="js/slick.min.js"></script>
    <!-- ====== isotope js ======= -->
    <script type="text/javascript" src="js/isotope.pkgd.min.js"></script>
    <!-- ====== lightbox js ======= -->
    <script type="text/javascript" src="js/lightbox.min.js"></script>
    <!-- ====== wow js ======= -->
    <script type="text/javascript" src="js/wow.min.js"></script>
    <!-- ====== waypoints js ======= -->
    <script type="text/javascript" src="js/jquery.waypoints.min.js"></script>
    <!-- ====== google map js ======= -->
    <script src="http://maps.google.com/maps/api/js?key=AIzaSyCMiBMwJko7vqlmlPOq-7wAW8DS5yNGz6Q"></script>
    <script type="text/javascript" src="js/gmap3.min.js"></script>
    <!-- ====== scrollUp js ======= -->
    <script type="text/javascript" src="js/jquery.scrollUp.min.js"></script>
    <!-- ====== active js ======= -->
    <script type="text/javascript" src="js/active.js"></script>
    <script type="text/javascript" src="js/main.js"></script>
    <!-- ====== google map active js ======= -->
    @Styles.Render("~/Content/css")
    
</head>
<body data-spy="scroll" data-target=".navbar">
    <header class="header-area">
        <div class="container">
            <div class="row">
                <div class="col-xs-5 col-sm-2 col-md-2">
                    <!-- Header logo -->
                    <div class="logo">
                        <a href="#">
                            <img src="~/Content/images/logo.png" alt="">
                        </a>
                    </div>
                </div>
                <div class="col-xs-7 col-sm-10 col-md-10">
                    <!-- Header menu -->
                    <nav class="navbar menu-part">
                        <ul class="nav nav-tabs" id="menu">
                            <li class="active"><a class="smooth-scroll" href="#top">home</a></li>
                            <li><a class="smooth-scroll" href="#about"><span class="separet">/</span> about diet</a></li>
                            <li><a class="smooth-scroll" href="/counter"><span class="separet">/</span> Counter</a></li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
    </header>

        @RenderBody()
        <hr />
    <footer class="fotter-bottom-area">
        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                    <div class="reserve-area">
                        <span>&copy;</span> All Rights Reserved 2017
                    </div> <!-- copyright part -->

                </div>
            </div>
        </div>
    </footer>


    @Scripts.Render("~/home/js/")
    @section scripts
    {

        @RenderSection("scripts", false)
    }
    @RenderSection("scripts", required: false)
</body>
</html>
